<!DOCTYPE html>
<html>
	<head>
		{% block metas %}{% endblock %}
		<title>
			{% block title %}{% endblock %}
		</title>
        {% block links %}{% endblock %}
		
        
        {% block scripts %}{% endblock %}

		</head>
		<body id="top">
			<header id="header" class="tt-nav transparent-header ">
				<div class="header-sticky semi-transparent">
					<div class="container">

						<div id="materialize-menu" class="menuzord">

							<a class="brand logo-brand" href="{{ path('app_home') }}"></a>
							<div id="mobile-menu">
								<div class="nav-wrapper" id="toggle">
									<span class="top"></span>
									<span class="middle"></span>
									<span class="bottom"></span>
								</div>
								<div class="icon-wrapper">
									<span class="account">
										<a href="{{ path('app_profile') }}">
											<i class="material-icons">&#xE7FD;</i>
										</a>
									</span>
								</div>
								<div class="nav-overlay" id="overlay">
									<nav class="overlay-menu">
										<ul>
											<li>
												<a href="{{ path('app_home') }}">Home</a>
											</li>
											<li>
												<a href="{{ path('app_jobs') }}">Jobs Offers</a>
											</li>
											<li>
												<a href="{{ path('app_company') }}">About Us</a>
											</li>
											<li>
												<a href="{{ path('app_contact') }}">Contact</a>
											</li>
											<li>-</li>
											<li>
												<a href="{{ path('app_profile') }}">Profile</a>
											</li>
											<li>
												<a href="{{ path('app_logout') }}">Logout</a>
											</li>
										</ul>
									</nav>
								</div>
							</div>
							<div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
								<ul class="menuzord-menu pull-right light" role="navigation">
									<li class=" dropdown">
										<a target="_self" href="{{ path('app_login') }}">
											<span>Login</span>
										</a>
									</li>
									<li class=" dropdown">
										<a target="_self" href="{{ path('app_register') }}" class="btn bn-lg gradient secondary btn-block waves-effect waves-light btn-register">
											<span>Sign Up</span>
										</a>

									</li>
								</ul>
								<ul class="menuzord-menu pull-right light" role="navigation">
									<li class="active dropdown">
										<a target="_self" href="{{ path('app_home') }}">
											<i class=""></i>
											<span>Home</span>
										</a>
									</li>
									<li class=" dropdown">
										<a target="_self" href="{{ path('app_jobs') }}">
											<i class=""></i>
											<span>Jobs offers</span>
										</a>
									</li>
									<li class=" dropdown">
										<a target="_self" href="{{ path('app_company') }}">
											<i class=""></i>
											<span>About us</span>
										</a>
									</li>
									<li class=" dropdown">
										<a target="_self" href="{{ path('app_contact') }}">
											<i class=""></i>
											<span>Contact</span>
										</a>
									</li>
								</ul>
							</div>
						</div>
					</div>
				</div>
			</header>
			{% block body %}{% endblock %}
			<!-- Footer -->
			<footer class="footer footer-four">
				<div class="primary-footer brand-bg text-center">
					<div class="container">

						<a href="#top" class="page-scroll btn-floating btn-large gradient secondary back-top waves-effect waves-light" data-section="#top">
							<i class="material-icons">&#xE316;</i>
						</a>

						<ul class="social-link tt-animate ltr mt-20">
							<li>
								<a href="#!" target="_blank">
									<i class="fa fa-facebook"></i>
								</a>
							</li>
							<li>
								<a href="#!" target="_blank">
									<i class="fa fa-linkedin"></i>
								</a>
							</li>
						</ul>

						<hr class="mt-10">

						<div class="row">
							<div class="col-md-12">
								<div class="brand footer-brand"></div>

								<span class="copy-text">© 2019 All right reserved - by
									<a href="https://idmkr.io/" target="_blank">IDMKR</a>
									-
									<a href="#!">Terms of use</a>

								</span>
								<div class="footer-intro">
									<p>Luxury Services is a leading professional recruitment consultancy specialising in the recruitment of permanent, contract and temporary positions on behalf of the world’s top employers.</p>
								</div>
							</div>
							<!-- /.col-md-12 -->
						</div>
						<!-- /.row -->
					</div>
					<!-- /.container -->
				</div>
				<!-- /.primary-footer -->

				<div class="secondary-footer brand-bg darken-2 text-center">
					<div class="container">
						<ul class="" role="navigation">
							<li class="active dropdown">
								<a target="_self" href="{{ path('app_home') }}">
									<span>Home</span>
								</a>
							</li>
							<li class=" dropdown">
								<a target="_self" href="{{ path('app_jobs') }}">
									<span>Jobs offers</span>
								</a>
							</li>
							<li class=" dropdown">
								<a target="_self" href="{{ path('app_company') }}">
									<span>About us</span>
								</a>
							</li>
							<li class=" dropdown">
								<a target="_self" href="{{ path('app_contact') }}">
									<span>Contact</span>
								</a>
							</li>
						</ul>
					</div>
					<!-- /.container -->
				</div>
				<!-- /.secondary-footer -->
			</footer>

			{% block scripts_two %}{% endblock %}
		</body>
	</html>

